<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
	<Appenders>
    	<Console name="Console">
      		<PatternLayout pattern="%d %-5p [%t] %C{2} (%F:%L) - %m%n"/>
    	</Console>
  	</Appenders>
  	<Loggers>
  		<Logger name="org.apache.log4j.xml" level="info"/>
    	<Root level="trace">
      		<AppenderRef ref="Console"/>
    	</Root>
  	</Loggers>
</Configuration>